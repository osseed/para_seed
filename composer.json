{
    "name": "osseed/para_seed",
    "description": "Installation profile for Drupal 8 supporting paragraphs with bootstrap layouts.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Tanmay Khedekar",
            "email": "tanmay@osseed.com"
        },
        {
          "type": "package",
          "package": {
            "name": "slick",
            "version": "v1.8.1",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/kenwheeler/slick.git",
              "type": "git",
              "reference": "master"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "blazy",
            "version": "1.8.2",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/dinbror/blazy.git",
              "type": "git",
              "reference": "master"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "dropzone",
            "version": "v5.7.2",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/enyo/dropzone.git",
              "type": "git",
              "reference": "master"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "masonry",
            "version": "v4.2.2",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/desandro/masonry.git",
              "type": "git",
              "reference": "master"
            }
          }
        },
        {
          "type": "package",
          "package": {
            "name": "imagesloaded",
            "version": "v4.1.4",
            "type": "drupal-library",
            "dist": {
              "url": "https://github.com/desandro/imagesloaded.git",
              "type": "git",
              "reference": "master"
            }
          }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
      {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
      }
    ],
    "extra": {
      "installer-types": [
        "bower-asset",
        "npm-asset"
      ],
      "installer-paths": {
        "web/core": [
          "type:drupal-core"
        ],
        "web/modules/contrib/{$name}": [
          "type:drupal-module"
        ],
        "web/libraries/{$name}": [
          "type:drupal-library"
        ],
        "web/profiles/contrib/{$name}": [
          "type:drupal-profile"
        ],
        "web/themes/contrib/{$name}": [
          "type:drupal-theme"
        ],
        "drush/contrib/{$name}": [
          "type:drupal-drush"
        ]
      }
    },
    "require": {
        "drupal/admin_toolbar": "^2.0",
        "drupal/config_update": "~1.0",
        "drupal/config_ignore": "~2.0",
        "drupal/swiftmailer": "~2.0",
        "drupal/varbase_media": "~7.0",
        "drupal/varbase_heroslider_media": "~7.0"
    }
}
