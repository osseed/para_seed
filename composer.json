{
    "name": "osseed/para_seed",
    "description": "Installation profile for Drupal 8 supporting paragraphs with bootstrap layouts.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Tanmay Khedekar",
            "email": "tanmay@osseed.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
      {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
      }
    ],
    "extra": {
      "installer-types": [
        "bower-asset",
        "npm-asset"
      ],
      "installer-paths": {
        "web/core": [
          "type:drupal-core"
        ],
        "web/modules/contrib/{$name}": [
          "type:drupal-module"
        ],
        "web/libraries/{$name}": [
          "type:drupal-library",
          "type:bower-asset",
          "type:npm-asset"
        ],
        "web/profiles/contrib/{$name}": [
          "type:drupal-profile"
        ],
        "web/themes/contrib/{$name}": [
          "type:drupal-theme"
        ],
        "drush/contrib/{$name}": [
          "type:drupal-drush"
        ],
        "web/libraries/masonry": ["npm-asset/masonry-layout"]
      },
      "drupal-libraries": {
        "library-directory": "web/libraries",
        "libraries": [
          {"name": "objectfitpolyfill", "package": "bower-asset/objectfitpolyfill"},
          {"name": "chartjs", "package": "bower-asset/chartjs"},
          {"name": "c3","package": "bower-asset/c3"},
          {"name": "d3", "package": "bower-asset/d3"},
          {"name": "dropzone","package": "npm-asset/dropzone"},
          {"name": "blazy", "package": "npm-asset/blazy"},
          {"name": "slick", "package": "npm-asset/slick-carousel"},
          {"name": "jquery-ui-touch-punch", "package": "bower-asset/jqueryui-touch-punch"},
          {"name": "masonry", "package": "npm-asset/masonry-layout"},
          {"name": "imagesloaded", "package": "npm-asset/imagesloaded"},
        ]
      }
    },
    "require": {
        "drupal/admin_toolbar": "^2.0",
        "drupal/config_update": "~1.0",
        "drupal/config_ignore": "~2.0",
        "drupal/swiftmailer": "~2.0",
        "drupal/varbase_media": "~7.0",
        "drupal/varbase_heroslider_media": "~7.0",
        "npm-asset/masonry-layout": "~4.0",
        "npm-asset/imagesloaded": "~4.0",
        "npm-asset/dropzone": "~4.0",
        "npm-asset/blazy": "~1.0",
        "npm-asset/slick-carousel": "~1.0"
    }
}
