{
    "name": "osseed/para_seed",
    "description": "Installation profile for Drupal 8 supporting paragraphs with bootstrap layouts.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Tanmay Khedekar",
            "email": "tanmay@osseed.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
      {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
      }
    ],
    "extra": {
      "installer-paths": {
        "web/core": [
          "type:drupal-core"
        ],
        "web/modules/contrib/{$name}": [
          "type:drupal-module"
        ],
        "web/libraries/{$name}": [
          "type:drupal-library"
        ],
        "web/profiles/contrib/{$name}": [
          "type:drupal-profile"
        ],
        "web/themes/contrib/{$name}": [
          "type:drupal-theme"
        ],
        "drush/contrib/{$name}": [
          "type:drupal-drush"
        ]
      }
    },
    "require": {
        "drupal/admin_toolbar": "^2.0",
        "drupal/config_update": "~1.0",
        "drupal/config_ignore": "~2.0",
        "drupal/swiftmailer": "~2.0",
        "drupal/paragraphs": "^1.10",
        "drupal/entityqueue": "~1.0",
        "drupal/field_group": "~3.0",
        "drupal/advanced_text_formatter": "~2.0",
        "drupal/blazy": "~2.0",
        "drupal/slick": "~2.0",
        "drupal/slick_views": "~2.0",
        "drupal/better_exposed_filters": "~5.0",
        "drupal/views_bulk_operations": "~3.0",
        "drupal/views_bulk_edit": "~2.0",
        "drupal/views_infinite_scroll": "~1.0",
        "drupal/imagemagick": "~3.0",
        "drupal/imageapi_optimize": "~2.0",
        "drupal/metatag": "^1.12",
        "drupal/entity_embed": "1.1",
        "drupal/inline_entity_form": "~1.0",
        "drupal/embed": "~1.0",
        "drupal/length_indicator": "~1.0",
        "drupal/advanced_text_formatter": "~2.0",
        "drupal/maxlength": "~1.0"
    }
}
